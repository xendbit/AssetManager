{
  "name": "assetmanager",
  "version": "1.0.0",
  "description": "Manage Assets using a Smart Contract",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "compile": "truffle compile",
    "clean": "rimraf build",
    "build": "npm run clean && npm run compile",
    "deploy": "truffle migrate --network dai",
    "deploy:dai": "truffle migrate --network dai --reset",
    "deploy:dev": "truffle migrate --network dev --reset",
    "deploy:ropsten": "truffle migrate --network ropsten --reset",
    "deploy:test": "truffle migrate --network test",
    "re-deploy": "npm run build && npm run deploy",
    "re-deploy:dev": "npm run build && npm run deploy:dev",
    "re-deploy:test": "npm run build && npm run deploy:test"
  },
  "author": "Akintayo Olusegun",
  "license": "ISC",
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.1.0",
    "eth-revert-reason": "^1.0.3",
    "keythereum": "^1.2.0",
    "solc": "^0.7.4",
    "truffle": "^5.1.49",
    "truffle-contract-size": "^2.0.0",
    "truffle-privatekey-provider": "^1.5.0",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2"
  }
}
